  <!-- question: 1 -->
  <question type="multichoice">
    <name>
      <text>Question 1</text>
    </name>
    <questiontext format="html">
      <text><![CDATA[<pre>Un fichier de volume initial Vi est compressé grâce au codage de Huffman. Après compression le fichier occupe un volume final Vf. Combien vaut alors le gain ?</pre>]]></text>
    </questiontext>
    <generalfeedback format="html">
      <text><![CDATA[<p>none</p>]]></text>
    </generalfeedback>
    <defaultgrade>3.0000000</defaultgrade>
    <penalty>0.3333333</penalty>
    <hidden>0</hidden>
    <single>true</single>
    <shuffleanswers>true</shuffleanswers>
    <answernumbering>abc</answernumbering>
    <answer fraction="0" format="html">
      <text><![CDATA[1. (Vf - Vi) / Vi]]></text>
      <feedback format="html">
        <text><![CDATA[<p>none</p>]]></text>
      </feedback>
    </answer>
    <answer fraction="100" format="html">
      <text><![CDATA[2. 1 - (Vf / Vi)]]></text>
      <feedback format="html">
        <text><![CDATA[<p>none</p>]]></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text><![CDATA[3. Aucune de ces propositions]]></text>
      <feedback format="html">
        <text><![CDATA[<p>none</p>]]></text>
      </feedback>
    </answer>
    <answer fraction="0" format="html">
      <text><![CDATA[4. (Vi - Vf) / Vf]]></text>
      <feedback format="html">
        <text><![CDATA[<p>none</p>]]></text>
      </feedback>
    </answer>
  </question>
